<% provide(:title, 'Find Friends') %>

<h1>Connect with Friends</h1>
<%= form_tag users_path, :method => 'get' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search User" %>
<%end %>

<%= will_paginate %>
<%# debugger %>
<ul class="users">
  <% @users.each do |user| %>
    <% if !current_user?(user) %>
      <%= render user %>
    <% end %>
  <% end %>
</ul>

<%= will_paginate %>